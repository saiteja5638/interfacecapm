VIEW "SELECTIONOPTIONS"
AS select 
APP_DB_INTERFACE_PARAMS.PARAMETER_NAME,
APP_DB_INTERFACE_TABLE.SERVICE_NAME,
APP_DB_INTERFACE_SERV_CONFIG.VALUE_ID
from
APP_DB_INTERFACE_TABLE

inner join APP_DB_INTERFACE_SERV_CONFIG on APP_DB_INTERFACE_TABLE.SERVICE_ID = APP_DB_INTERFACE_SERV_CONFIG.SERVICE_ID
inner join APP_DB_INTERFACE_PARAMS on APP_DB_INTERFACE_SERV_CONFIG.PARAMETER_ID = APP_DB_INTERFACE_PARAMS.PARAMETER_ID


